{
    "compilerOptions": {
        "target": "ES2015",
        "lib": ["DOM", "ESNext"],
        "allowJs": true,
        "module": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": false,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "moduleResolution": "node"
    },
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Base"
}
